#!/usr/bin/execlineb -S0
if { redirfd -c 1 /etc/group  cat /root/group }
if { redirfd -c 1 /etc/passwd cat /root/passwd }
if { redirfd -c 1 /etc/shadow cat /root/shadow }
if { redirfd -c 1 /etc/apk/world cat /root/world }
if { apk -U fix }
if { elglob apkix /var/cache/apk/* rm -f $apkix }
rm /root/group /root/passwd /root/shadow /root/world $0
